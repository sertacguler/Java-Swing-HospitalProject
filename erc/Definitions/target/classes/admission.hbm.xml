<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.erc.definitions.admission.entity.AdmissionDTO"
		table="AQADMISSION">

		<id name="admissionid">
			<generator class="increment"></generator>
		</id>

		<property name="patientid"></property>
		<property name="organizationid"></property>
		<property name="staffId"></property>
		<property name="admissionDate"></property>
		<property name="admissionType"></property>
		<property name="admissionNo"></property>
		<property name="admissionStatus"></property>
		<property name="status"></property>
		<property name="cancelReason"></property>

	</class>
</hibernate-mapping>