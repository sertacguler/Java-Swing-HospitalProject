<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ebc.admission.serviceType.entity.ServiceTypeDto"
		table="AQSERVICETYPE">

		<id name="serviceTypeId" type="java.lang.Long">
			<generator class="increment"></generator>
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>
		<property name="status"  length="1" type="java.lang.Boolean" >
			<column name="status" />
		</property>

	</class>
</hibernate-mapping>